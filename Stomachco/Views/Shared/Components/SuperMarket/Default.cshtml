@model IEnumerable<SuperMarket>

<div class="rest" id="rest">
	<div class="container">

		<div class="restlogo">
			<p>SuperMarkets</p>
		</div>

		<div class="row">
			@foreach (var item in Model)
			{

				<div class="package">
					<a asp-action="view3" asp-controller="Home" class="comp">
						<img width="100" height="100" src="@Url.Content(item.SMImage)" alt="Restaurant Photo" />
						<div class="description">
							<p class="restname">@item.SMName</p>
				
						</div>
					</a>
				</div>
			}



		</div>
	</div>
</div>